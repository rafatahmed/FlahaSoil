  <text x="110" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">Clay</text>
  <text x="140" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">Sand</text>
  <text x="170" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">Silt</text>
  <text x="200" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">OM</text>
  <text x="230" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">pH</text>
  <text x="260" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">FC</text>
  
  <!-- Title -->
  <text x="200" y="30" text-anchor="middle" font-family="Arial" font-size="16" font-weight="bold" fill="#2D5430">Advanced Soil Analysis</text>
  
  <!-- Connection lines -->
  <line x1="120" y1="195" x2="140" y2="150" stroke="#FF5722" stroke-width="2" stroke-dasharray="3,3"/>
  <line x1="200" y1="215" x2="200" y2="150" stroke="#FF5722" stroke-width="2" stroke-dasharray="3,3"/>
  <line x1="280" y1="200" x2="250" y2="150" stroke="#FF5722" stroke-width="2" stroke-dasharray="3,3"/>
</svg>
