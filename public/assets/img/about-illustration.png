<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>
  
  <!-- Soil layers -->
  <rect x="50" y="180" width="300" height="30" fill="#8D6E63"/>
  <rect x="50" y="210" width="300" height="40" fill="#6D4C41"/>
  <rect x="50" y="250" width="300" height="30" fill="#5D4037"/>
  
  <!-- Surface -->
  <rect x="50" y="170" width="300" height="10" fill="#4CAF50"/>
  
  <!-- Analysis points -->
  <circle cx="120" cy="200" r="5" fill="#FF5722"/>
  <circle cx="200" cy="220" r="5" fill="#FF5722"/>
  <circle cx="280" cy="205" r="5" fill="#FF5722"/>
  
  <!-- Data visualization -->
  <rect x="80" y="50" width="240" height="100" fill="white" stroke="#ddd" stroke-width="1"/>
  
  <!-- Chart bars -->
  <rect x="100" y="100" width="20" height="30" fill="#1976D2"/>
  <rect x="130" y="90" width="20" height="40" fill="#4CAF50"/>
  <rect x="160" y="110" width="20" height="20" fill="#FF9800"/>
  <rect x="190" y="85" width="20" height="45" fill="#9C27B0"/>
  <rect x="220" y="95" width="20" height="35" fill="#F44336"/>
  <rect x="250" y="105" width="20" height="25" fill="#607D8B"/>
  
