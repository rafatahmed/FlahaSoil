<?xml version="1.0" encoding="UTF-8"?>
<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="200" fill="#f8f9fa"/>
  
  <!-- Field representation -->
  <rect x="20" y="40" width="260" height="120" fill="#4CAF50" opacity="0.3"/>
  
  <!-- Grid pattern for crops -->
  <g stroke="#2D5430" stroke-width="1" opacity="0.6">
    <line x1="50" y1="40" x2="50" y2="160"/>
    <line x1="80" y1="40" x2="80" y2="160"/>
    <line x1="110" y1="40" x2="110" y2="160"/>
    <line x1="140" y1="40" x2="140" y2="160"/>
    <line x1="170" y1="40" x2="170" y2="160"/>
    <line x1="200" y1="40" x2="200" y2="160"/>
    <line x1="230" y1="40" x2="230" y2="160"/>
    <line x1="260" y1="40" x2="260" y2="160"/>
  </g>
  
  <!-- Tractor/equipment -->
  <rect x="220" y="120" width="40" height="20" fill="#FFD700"/>
  <circle cx="230" cy="145" r="8" fill="#333"/>
  <circle cx="250" cy="145" r="8" fill="#333"/>
  
  <!-- Title -->
  <text x="150" y="25" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2D5430">Precision Agriculture</text>
  
  <!-- Data points -->
  <circle cx="80" cy="80" r="4" fill="#FF5722"/>
  <circle cx="150" cy="100" r="4" fill="#FF5722"/>
  <circle cx="220" cy="70" r="4" fill="#FF5722"/>
  
  <!-- Legend -->
  <text x="20" y="185" font-family="Arial" font-size="10" fill="#666">Soil Analysis Points</text>
</svg>
