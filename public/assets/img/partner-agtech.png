<?xml version="1.0" encoding="UTF-8"?>
<svg width="120" height="60" viewBox="0 0 120 60" xmlns="http://www.w3.org/2000/svg">
  <!-- Tech circuit pattern -->
  <rect x="20" y="20" width="80" height="20" fill="#E8F5E8" rx="10"/>
  
  <!-- Circuit lines -->
